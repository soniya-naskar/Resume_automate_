{
    "manifest_version": 3,
    "name": "Resume_Automate_Filler",
    "version": "1.0",
    "description": "Automatically fills in resume details on job application forms with user-defined data.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
     
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  